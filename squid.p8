-- translate a 2d x,y coordinate to a binary idx
function vectoidx(vec)
	local x = flr(vec.x) >>> 11
	local y = flr(vec.y) --<< 11
	return x | y
end

function idxtovec(idx)
	local tx = (idx << 11) & 0b11111111111
 local ty = idx & 0b1111111111
 return {x=tx,y=ty}
end
